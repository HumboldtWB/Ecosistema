---
import Layout from "../layouts/Layout.astro";
---

<Layout title="Directorio | Ecosistema Humboldt">
  <main>
    <header class="link-card-grid2">
      <img src="img/Banner-directorio.png" width="1500" />
    </header>
    <div class="max-w-[80%] mx-auto">
      <span
        onclick="history.back()"
        class="text-azul-humboldt text-base mb-10 block cursor-pointer"
      >
        <img src="img/volver.svg" alt="volver" width="20" class="inline mr-3" />
        Volver
      </span>
    </div>
    <!-- Inicio Directorio direccion General  -->
    <div
      id="accordion-collapse-1"
      data-accordion="collapse"
      class="grid gap-4 max-w-[80%] mx-auto mb-10"
    >
      <h2 id="accordion-collapse-heading-1">
        <button
          type="button"
          class="flex items-center justify-between w-full p-5 border-b-2 gap-3"
          data-accordion-target="#accordion-collapse-body-1"
          aria-expanded="false"
          aria-controls="accordion-collapse-body-1"
        >
          <span class="text-4xl text-azul-light font-bold">
            Dirección General
          </span>
          <svg
            data-accordion-icon
            class="w-3 h-3 rotate-180 shrink-0"
            aria-hidden="true"
            xmlns="http://www.w3.org/2000/svg"
            fill="none"
            viewBox="0 0 10 6"
          >
            <path
              stroke="currentColor"
              stroke-linecap="round"
              stroke-linejoin="round"
              stroke-width="2"
              d="M9 5 5 1 1 5"></path>
          </svg>
        </button>
      </h2>
      <div
        id="accordion-collapse-body-1"
        class="hidden"
        aria-labelledby="accordion-collapse-heading-1"
      >
        <div class="flex flex-col items-center space-y-8">
          <div class="flex flex-col items-center space-y-6">
            <div
              class="bg-white shadow-lg rounded-2xl p-4 flex flex-col items-center"
            >
              <img
                class="block mx-auto w-[150px] h-[150px] object-contain mb-5 rounded-full"
                src="img/directorio/direccion/Hernando Garcia Martinez.jpg"
              />
              <h2 class="text-xl font-semibold">Hernando García</h2>
              <p class="text-gray-500">Director General</p>
              <p class="text-gray-500">hgarcia@humboldt.org.co</p>
            </div>
            <div class="grid grid-cols-3 gap-6">
              <div
                class="bg-white shadow-lg rounded-2xl p-4 flex flex-col items-center"
              >
